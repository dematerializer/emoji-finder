{
  "name": "emoji-quickfinder",
  "version": "0.0.0",
  "private": true,
  "description": "emoji quickfinder",
  "author": "Andreas Tietz https://github.com/dematerializer",
  "bin": "lib/cli.js",
  "main": "lib/index.js",
  "scripts": {
    "update-local-deps": "rm -rf node_modules/unicode-emoji-*; npm i",
    "start": "node cli.js",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --require ./test-env --compilers js:babel-core/register src/**/*.test.js",
    "posttest": "npm run lint",
    "lint": "eslint --ignore-path .gitignore .",
    "prepublish": "npm run test && npm run build",
    "build": "rm -rf lib; mkdir lib; babel src -d lib --ignore **/*.test.js"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "copy-paste": "^1.3.0",
    "fuzzaldrin": "^2.1.0",
    "has-ansi": "^2.0.0",
    "log-update": "^1.0.2",
    "redux": "^3.5.2",
    "redux-saga": "^0.12.0",
    "reselect": "^2.5.3",
    "unicode-emoji-annotations": "../unicode-emoji-annotations",
    "unicode-emoji-data": "../unicode-emoji-data"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-eslint": "^7.1.0",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-polyfill": "^6.13.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.0.1",
    "mocha": "^3.0.2",
    "nyc": "^8.3.0",
    "strip-ansi": "^3.0.1"
  }
}
