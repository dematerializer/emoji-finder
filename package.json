{
  "name": "emoji-finder",
  "version": "0.0.0",
  "private": true,
  "description": "Quickly find and copy emoji to the clipboard via the CLI",
  "keywords": [
    "cli",
    "emoji",
    "emojis",
    "emoticon",
    "search",
    "find",
    "fuzzy",
    "copy",
    "clipboard",
    "internationalized",
    "i18n"
  ],
  "author": "Andreas Tietz (https://github.com/dematerializer)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dematerializer/emoji-finder.git"
  },
  "bugs": {
    "url": "https://github.com/dematerializer/emoji-finder/issues"
  },
  "homepage": "https://github.com/dematerializer/emoji-finder",
  "bin": "lib/cli.js",
  "main": "lib/index.js",
  "scripts": {
    "start": "node cli.js",
    "precommit": "npm run validate",
    "prepush": "npm run validate",
    "prepublish": "npm run validate && npm run build",
    "validate": "npm run cover && npm run check-coverage",
    "cover": "nyc --reporter=lcov --reporter=text npm test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "NODE_ENV=test mocha --require ./test-env --compilers js:babel-core/register src/**/*.test.js",
    "posttest": "npm run lint",
    "lint": "eslint --ignore-path .gitignore .",
    "build": "rm -rf lib; mkdir lib; babel src -d lib --ignore **/*.test.js"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "chalk": "1.1.3",
    "copy-paste": "1.3.0",
    "fuzzaldrin": "2.1.0",
    "has-ansi": "2.0.0",
    "log-update": "1.0.2",
    "punycode": "2.0.1",
    "redux": "3.6.0",
    "redux-saga": "0.13.0",
    "regenerator-runtime": "0.9.6",
    "reselect": "2.5.4",
    "unicode-emoji-annotations": "1.0.0",
    "unicode-emoji-data": "2.0.4"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.0",
    "babel-plugin-istanbul": "3.0.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "eslint": "3.10.2",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-plugin-import": "2.2.0",
    "husky": "0.12.0",
    "mocha": "3.1.2",
    "nyc": "9.0.1",
    "strip-ansi": "3.0.1"
  }
}
